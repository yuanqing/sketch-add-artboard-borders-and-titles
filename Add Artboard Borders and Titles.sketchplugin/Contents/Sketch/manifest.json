{
  "name": "Add Artboard Borders and Titles",
  "description": "A Sketch plugin to automatically add borders and titles to artboards",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-add-artboard-borders-and-titles",
  "version": "0.5.0",
  "identifier": "yuanqing.sketch-add-artboard-borders-and-titles",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-add-artboard-borders-and-titles/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Add Artboard Borders",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-borders",
      "script": "plugin.js",
      "shortcut": "ctrl option cmd b",
      "handler": "add-artboard-borders/add-artboard-borders"
    },
    {
      "name": "Settings",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.settings",
      "script": "plugin.js",
      "handler": "add-artboard-borders/settings"
    },
    {
      "name": "Add Artboard Titles",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-titles",
      "script": "plugin.js",
      "shortcut": "ctrl option cmd t",
      "handler": "add-artboard-titles/add-artboard-titles"
    },
    {
      "name": "Settings",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.settings",
      "script": "plugin.js",
      "handler": "add-artboard-titles/settings"
    }
  ],
  "menu": {
    "title": "Add Artboard Borders and Titles",
    "items": [
      "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-borders",
      "yuanqing.sketch-add-artboard-borders-and-titles.settings",
      "-",
      "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-titles",
      "yuanqing.sketch-add-artboard-borders-and-titles.settings"
    ]
  }
}
