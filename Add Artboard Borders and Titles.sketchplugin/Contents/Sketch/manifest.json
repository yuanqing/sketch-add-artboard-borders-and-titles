{
  "name": "Add Artboard Borders and Titles",
  "description": "A Sketch plugin to automatically add borders and titles to artboards",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-add-artboard-borders-and-titles",
  "version": "0.7.0",
  "identifier": "yuanqing.sketch-add-artboard-borders-and-titles",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-add-artboard-borders-and-titles/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Add Artboard Borders",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-borders.add-artboard-borders",
      "script": "plugin.js",
      "handler": "add-artboard-borders/add-artboard-borders"
    },
    {
      "name": "Add Artboard Titles",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-titles.add-artboard-titles",
      "script": "plugin.js",
      "handler": "add-artboard-titles/add-artboard-titles"
    },
    {
      "name": "Settings for Add Artboard Borders",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-borders.settings",
      "script": "plugin.js",
      "handler": "add-artboard-borders/settings"
    },
    {
      "name": "Settings for Add Artboard Titles",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-titles.settings",
      "script": "plugin.js",
      "handler": "add-artboard-titles/settings"
    },
    {
      "name": "Reset Settings",
      "identifier": "yuanqing.sketch-add-artboard-borders-and-titles.reset-settings",
      "script": "plugin.js",
      "handler": "reset-settings"
    }
  ],
  "menu": {
    "title": "Add Artboard Borders and Titles",
    "items": [
      "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-borders.add-artboard-borders",
      "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-titles.add-artboard-titles",
      "-",
      {
        "title": "Settings",
        "items": [
          "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-borders.settings",
          "yuanqing.sketch-add-artboard-borders-and-titles.add-artboard-titles.settings",
          "-",
          "yuanqing.sketch-add-artboard-borders-and-titles.reset-settings"
        ]
      }
    ]
  }
}
